<div id="loginpage">
    <h1 id="registh1">Login Here</h1>
    <%- include('messages') %>
    <form action="/login" method="POST">
        <label class="text" for="username">USERNAME</label><br>
        <input placeholder="Enter Username" id="username" name="username" type="text"><br>
        <label class="text" for="password">PASSWORD</label><br>
